* { margin: 0; padding: 0; }
body { margin: 0; padding: 0px; border: 0; }
#container {
	width: 960px;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	text-align: left;
	position: absolute;
	left: 0px;
}
#header { height: 94px; }
#content {
	width: 960px;
	padding: 0px 0px 0px 20px;
	margin: 0px 0px 0px 0px;
	min-height: 500px;
}
#content ul { margin: 0px 0 10px 20px; }
#content li { margin-bottom: 2px; }
#content_footer { clear: both; }
#footer {
	border-top: 1px solid #EEEEEE;
	clear: both;
	margin: 30px 0 10px 20px;
	overflow: hidden;
	padding: 10px 0 20px 0;
	position: absolute;
}
#footer .footer { width: 960px; }
#footer .assist {
	float: right;
	left: 20px;
	position: relative;
	top: -5px;
}
#footer .text_links { float: left; }
#footer h2 { color: #A6A6A6; }
#footer a { margin: 0 15px 0 0px; }
#footer a.print {
	background: url(/images_new/icon_print.png) top left no-repeat;
	left: 520px;
	margin: 10px 0px;
	padding: 2px 0px 1px 20px;
	position: relative;
}
#footer a.rss_feed {
	background: url(/images/feedicon12.gif) top left no-repeat;
	margin: 0;
	padding: 1px 10px 1px 15px;
}
html { height: 100%; }
.clear_left { clear: left; }
