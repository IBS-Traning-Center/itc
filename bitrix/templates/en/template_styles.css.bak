/*********** #left_menu styles ***********/
#left_menu {
	float: left;
	width: 200px;
	margin: 11px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	min-height: 5px;
}
#left_menu li a {
	padding: 3px 5px 3px 5px;
	display: block;
	text-decoration: none;
}
#left_menu li.indent a { padding: 3px 0 3px 5px; margin: 0px 0 0px -5px; }
#left_menu li.indent {
	list-style-image: url(images/arr1.png);
	list-style-position: outside;
	margin: 0px 0 0px 5px;
}
#left_menu li.vert_indent { padding-top: 10px; }
.ie #left_menu li.indent a { position: relative; overflow: auto; left: -5px; }
.ie #left_menu li.indent { margin: 0px 0px 0px 9px; }
#left_menu li.minus { list-style-image: url(images/arr2.png); }
#left_menu li a:hover { background-color: #F9F9F9; color: #FC711B; }
#left_menu ul { padding: 0; margin: 0; }
.ie #left_menu ul { margin: 0 0 0 14px; position: relative; left: -11px; }
#left_menu li {
	list-style: none;
	margin-bottom: 0px;
	padding: 0 0 5px 0;
	margin: 0;
}
#left_menu li.selected a {
	color: #FC711B;
	margin-bottom: 0px;
	font-weight: normal;
}
#left_menu li.vip { margin-left: 20px; }
#left_menu li.selectedvip a { margin-left: 20px; color: #FC711B; }
#sectionlinks li.selectedvip a { color: #FC711B; font-weight: bold; }
/*********** #left_menu styles endddddd ***********/
