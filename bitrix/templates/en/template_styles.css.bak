/************* #slogan block styles *************/
.box_headline {
	margin: -30px 20px 0px 0px;
	padding: 10px;
	height: 163px;
	border: #9B9BA3 1px solid;
}
.player { position: absolute; top: 305px; left: 450px; }
.player ul { padding: 6px; }
.player li { display: inline; }
.opera .player ul { padding: 0px; }
.opera .player li { float: left; }
.opera .player a { display: block; margin: 0px 2px 0px 2px; }
.player a { padding: 5px 10px 5px 10px; }
.player a:link, .player a:visited {
	color: #FFFFFF;
	background-color: #000000;
	text-decoration: none;
	opacity: .70; /* Standard style for transparency */
	-moz-opacity: .70; /* Transparency for older Mozillas */
	filter: alpha(opacity=70); /* Transparency for IE */
}
.player a:hover {
	color: #FF7A00;
	background-color: #B7B8B8;
	text-decoration: none;
}
.player a.active, .player a:visited.active {
	background-color: #FFFFFF;
	color: #FF7A00;
	font-weight: bold;
	opacity: .70; /* Standard style for transparency */
	-moz-opacity: .70; /* Transparency for older Mozillas */
	filter: alpha(opacity=70); /* Transparency for IE */
	z-index: 999;
}
/************* #slogan block styles endddddd*************/
