/* ----------- My Form ----------- */
.myform { margin: 0; padding: 14px; overflow: auto; width: 390px; }
/* ----------- stylized (form style)  ----------- */
#stylized { background: #EBF4FB; }
#stylized h1 { font-size: 14px; font-weight: bold; margin-bottom: 8px; }
#stylized p {
	font-size: 11px;
	color: #666666;
	margin-bottom: 20px;
	border-bottom: solid 1px #B7DDF2;
	padding-bottom: 10px;
}
#stylized label {
	display: block;
	font-weight: bold;
	text-align: right;
	width: 140px;
	float: left;
}
#stylized label.error {
	display: block;
	font-weight: normal;
	text-align: left;
	width: 100%;
	color: #FF0000;
	vertical-align: top;
	margin: -15px 0px 10px 150px;
	padding: 0px 0px 0px 0px;
}
.ie6 #stylized { margin: 10px 0px 0px 0px; }
.ie6 #stylized label.error { width: 40%; margin: -15px 0px 10px 80px; }
#stylized label.red { color: #FF0000; }
#stylized .small {
	color: #666666;
	display: block;
	font-size: 11px;
	font-weight: normal;
	text-align: right;
	width: 140px;
}
#stylized input {
	float: left;
	font-size: 12px;
	padding: 4px 2px;
	border: solid 1px #AACFE4;
	width: 180px;
	margin: 2px 0 15px 10px;
}
#stylized input.but { width: 125px; margin-left: 150px; }
#stylized textarea {
	float: left;
	font-size: 12px;
	padding: 4px 2px;
	border: solid 1px #AACFE4;
	width: 250px;
	height: 180px;
	margin: 2px 0 20px 10px;
	font-family: Arial;
}
#stylized button {
	float: left;
	width: 125px;
	height: 31px;
	text-align: center;
	line-height: 31px;
	color: #FFFFFF;
	font-size: 11px;
	font-weight: bold;
	margin: 2px 0 20px 10px;
}
#sitesearch         /* search.html*/ { width: 700px; }
.sitesearchitem {
	margin: 0 0 20px 0;
	padding: 0 0 20px 0;
	border-bottom: 1px solid #EEEEEE;
}
.sitesearchitem a.title { font-size: 140%; font-weight: bold; }
.sitesearchitem a.rel { color: #A6A6A6; font-size: normal; font-weight: normal; }
#sitemap         /* map.html*/ { }
ul.map-level-0 a { font-weight: bold; }
ul.map-level-1 a, ul.map-level-2 a, ul.map-level-3 a { font-weight: normal; }
.map-columns td { vertical-align: top; }
